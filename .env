VELOX_USER=admin
VELOX_PASSWORD=admin
VELOX_ROLE=administrator

# this package defaults to port 443, so no port necessary unless you change it
VELOX_SERVER_URL=https://my.server.url/
VELOX_FRONTEND_HOSTNAME=my.server.url

# only set these if you want to use CAs and TLS certs that are not auto-generated
VELOX_TLS_CERT=/opt/velociraptor/certs/server.crt
VELOX_TLS_KEY=/opt/velociraptor/certs/server.key
VELOX_TLS_CA=/opt/velociraptor/certs/ca.crt